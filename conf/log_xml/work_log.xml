<seelog>
    <outputs formatid="ad_log">
        <console/>
        <filter levels="info">
            <buffered formatid="ad_log" size="10000" flushperiod="100">
                <rollingfile type="date" filename="/Users/hepengfei/Documents/log/direct_ad/info/info.log" datepattern="2006-01-02-15" maxrolls="72"/>
            </buffered>
        </filter>
        <filter levels="debug">
            <buffered formatid="ad_log" size="10000" flushperiod="100">
                <rollingfile type="date" filename="/Users/hepengfei/Documents/log/direct_ad/others/debug.log" datepattern="2006-01-02-15" maxrolls="72"/>
            </buffered>
        </filter>
        <filter levels="warn">
            <buffered formatid="ad_log" size="10000" flushperiod="100">
                <rollingfile type="date" filename="/Users/hepengfei/Documents/log/direct_ad/others/warn.log" datepattern="2006-01-02-15" maxrolls="72"/>
            </buffered>
        </filter>
        <filter levels="error">
            <buffered formatid="ad_log" size="10000" flushperiod="100">
                <rollingfile type="date" filename="/Users/hepengfei/Documents/log/direct_ad/others/error.log" datepattern="2006-01-02-15" maxrolls="72"/>
            </buffered>
        </filter>
        <filter levels="critical">
            <buffered formatid="ad_log" size="10000" flushperiod="100">
                <rollingfile type="date" filename="/Users/hepengfei/Documents/log/direct_ad/others/critical.log" datepattern="2006-01-02-15" maxrolls="72"/>
            </buffered>
        </filter>
    </outputs>
    <formats>
        <format id="ad_log" format="%Date %Time [%LEVEL] [%Func] [%RelFile:%Line] %Msg%n"/>
    </formats>
</seelog>
